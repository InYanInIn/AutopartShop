<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="180.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="30.0" layoutY="30.0" style="-fx-font-size: 14px;" text="Enter New Discount(%):">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="quantityField" layoutX="30.0" layoutY="55.0" prefWidth="240.0" text="12" />

        <Button fx:id="submitButton" layoutX="30.0" layoutY="110.0" mnemonicParsing="false" prefWidth="100.0" text="Submit" />

        <Button fx:id="cancelButton" layoutX="170.0" layoutY="110.0" mnemonicParsing="false" prefWidth="100.0" text="Cancel" />
    </children>
</AnchorPane>
